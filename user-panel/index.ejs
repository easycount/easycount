<div id="superDiv" onClick="escapar()" style="z-index:9999;position:fixed; background-color:rgba(0,0,0, 0.6); width:100%; height:100%;display:none">
    <iframe class="preview-pane" type="application/pdf" width="100%" height="650" frameborder="0" style="position:relative;z-index:500"></iframe>
    <button class="btn btn-danger" onClick="escapar();" style="z-index:100000000; position: fixed; right:0; top: 0; width: 30px;">x</button>
</div>

<div>
    <!-- Esto lo hacemos para recuperar la sesión y almacenar sus valores en un controlador de AngularJS -->
    <% if (user){ %>
    <input id="hiddenId" type="hidden" value="<%= user._id %>" />
    <input id="hiddenName" type="hidden" value="<%= user.name %>" />
    <input id="hiddenEmail" type="hidden" value="<%= user.email %>" />

    <% if(foto){ %>

    <input id="hiddenPhoto" type="hidden" value="<%= foto %>" />
    <% }%>

    <% }%>
    <div ui-view></div>

</div>

